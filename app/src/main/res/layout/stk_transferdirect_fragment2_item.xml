<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_row"
        android:layout_width="32dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="行号"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_mats"
        android:layout_width="120dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="物料代码\n名称\n规格"
        android:textColor="#000000"
        android:textSize="@dimen/_11sp" />


    <TextView
        android:id="@+id/tv_nums"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_blue2"
        android:gravity="center"
        android:text="应发数\n实发数"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_stockAP"
        android:layout_width="100dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_blue5b"
        android:gravity="center"
        android:hint="请选择调出仓库"
        android:textColor="#000000"
        android:textSize="@dimen/_11sp" />

    <TextView
        android:id="@+id/tv_stockAP2"
        android:layout_width="100dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_blue5b"
        android:gravity="center"
        android:hint="请选择调入仓库"
        android:textColor="#000000"
        android:textSize="@dimen/_11sp" />

    <TextView
        android:id="@+id/tv_batch_seqNo"
        android:layout_width="86dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="批号\n序列号"
        android:textColor="#000000"
        android:textSize="12sp" />

</LinearLayout>
